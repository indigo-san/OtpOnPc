             xmlns:sty="using:FluentAvalonia.Styling">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="/Styles/LiteNav.axaml" />
            </ResourceDictionary.MergedDictionaries>
